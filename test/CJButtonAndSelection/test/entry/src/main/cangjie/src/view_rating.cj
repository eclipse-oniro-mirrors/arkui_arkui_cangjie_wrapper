package ohos_app_cangjie_entry

import ohos.base.*
import ohos.arkui.component.*
import ohos.arkui.state_management.*
import ohos.arkui.state_macro_manage.*
import std.collection.{ArrayList, HashMap}

@Entry
@Component
class View_Rating {
    @State
    var rating: Float64 = 3.5
    @State
    var rating2: Float64 = 4.0
    @State
    var backPng: String = "data:image/png;base64,"
    @State
    var forePng: String = "data:image/png;base64,"
    @State
    var secondPng: String = "data:image/png;base64,"

    func build() {
        Column() {
            Rating(rating: rating, indicator: false)
                .stars(5)
                .stepSize(-0.5)
                .stepSize(0.5)
                .starStyle(
                    backgroundUri: backPng, // common目录与pages同级
                    foregroundUri: forePng,
                    secondaryUri: secondPng
                )
                .margin(24)
                .onChange({
                    value: Float64 => this.rating = value
                })
                .id("my_rating")

            Text("current score is ${this.rating}").fontSize(16).fontColor(0x182431).margin(16).id("my_score")

            Rating(rating: rating2, indicator: false)
                .stars(5)
                .stepSize(1.0)
                .starStyle(
                    backgroundUri: backPng, // common目录与pages同级
                    foregroundUri: forePng,
                    secondaryUri: secondPng
                )
                .margin(24)
                .onChange({
                    e =>
                })

            Text("current score is ${this.rating2}").fontSize(16).fontColor(0x182431).margin(16)
        }.width(100.percent).padding(top: 5)
    }
}
