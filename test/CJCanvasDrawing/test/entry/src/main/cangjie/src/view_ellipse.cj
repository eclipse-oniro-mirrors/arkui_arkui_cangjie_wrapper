package ohos_app_cangjie_entry

import ohos.base.*
import ohos.arkui.component.*
import ohos.arkui.state_management.*
import ohos.arkui.state_macro_manage.*

@Entry
@Component
class ViewEllipse {
    func build() {
        Column {
            Ellipse(width: 150, height: 80).id("ellipse_1")
            // 绘制一个 150 * 100 、线条为蓝色的椭圆环
            Ellipse()
                .width(150)
                .height(100)
                .fillOpacity(0.0)
                .stroke(Color.Red)
                .strokeWidth(3)
                .id("ellipse_2")
        }
    }
}
